{
  "name": "nutricoach-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "echo 'Building NutriCoach without Stripe dependencies...' && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless'",
    "lighthouse:ci": "lhci autorun",
    "perf:audit": "npm run build && npm run start & sleep 5 && npm run lighthouse && kill %1",
    "perf:monitor": "npm run analyze && npm run lighthouse:ci"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.4.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.1",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "dotenv": "^16.6.0",
    "next": "^15.1.3",
    "postcss": "^8.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.2",
    "stripe": "^17.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.6.3",
    "web-push": "^3.6.7",
    "framer-motion": "^11.0.0",
    "workbox-webpack-plugin": "^7.0.0",
    "workbox-window": "^7.0.0",
    "@react-email/components": "^0.0.25",
    "@react-email/tailwind": "^0.1.0",
    "react-email": "^3.0.1",
    "resend": "^4.0.0",
    "handlebars": "^4.7.8",
    "juice": "^11.0.0",
    "next-pwa": "^5.6.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.3.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.1.3",
    "lighthouse": "^12.7.0",
    "vitest": "^1.6.1",
    "worker-loader": "^3.0.8"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
