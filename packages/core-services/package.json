{
  "name": "@nutricoach/core-services",
  "version": "1.0.0",
  "description": "Core business services and utilities for NutriCoach ecosystem",
  "main": "index.ts",
  "types": "index.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
